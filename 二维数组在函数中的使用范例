#include<stdio.h>
#include<string.h>
#include <stdlib.h>

int sec(int* score, char names[][100], int n);
int thi(int* score, char names[][100], int n);
int func(const char* p1, const char* p2);

int main() {
    int n;
    scanf("%d", &n);
    scanf("%d", &n);
    
    int a[100];
    char b[100][100];
    
    for (int i = 0; i < n; i++) {
        scanf("%s", b[i]);
        scanf("%d", &a[i]);
    }
    
    int order;
    while (1) {
        scanf("%d", &order);
        if (order == 0) break;
        if (order == 2) {
            sec(a, b, n);  // 直接传递数组
        }
        if (order == 3) {
            thi(a, b, n);
        }
    }
    return 0;
}

int sec(int* score, char names[][100], int n) {
    while (1) {
        int count = 0;
        for (int i = 0; i < n-1; i++) {
            if (score[i] < score[i+1] || 
                (score[i] == score[i+1] && func(names[i], names[i+1]))) {
                // 交换score
                int temp_score = score[i];
                score[i] = score[i+1];
                score[i+1] = temp_score;
                
                // 交换names
                char temp_name[100];
                strcpy(temp_name, names[i]);
                strcpy(names[i], names[i+1]);
                strcpy(names[i+1], temp_name);
                
                count++;
            }
        }
        if (count == 0) break;
    }
    return 0;
}

int thi(int* score, char names[][100], int n) {
    for (int i = 0; i < n; i++) {
        printf("%s %d\n", names[i], score[i]);
    }
    return 0;
}

int func(const char* p1, const char* p2) {
    return strcmp(p1, p2) > 0;  // p1在字典序中是否大于p2
}
