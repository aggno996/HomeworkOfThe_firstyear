#include <stdio.h>
#include <string.h>
int main()
{
    char achar [200];
    char bchar [200];
    gets(achar);
    gets(bchar);
    int number1 = strlen(achar);
    int number2 = strlen(bchar);
    if(achar[0] =='0'|| bchar[0]=='0'){
        printf("0");
        return 0;
    }
    int a[200];
    int b[200];
    for(int k =0;k<number1;k++)
    {
          a[k]=achar[number1-1-k] - '0';
    }
    for(int k =0;k<number2;k++)
    {
          b[k]=bchar[number2-1-k] - '0';
    }
    int i =0;
    int s=0,t=0;
    int result[450];
    for(int k =0;k<450;k++){
        result[k]=0;
    }
    for(i=0;i<number1;i++)
    {
        for(int j =0;j<number2;j++)
        {
            result[s]=result[s]+b[j]*a[i];
            result[s+1]=result[s+1]+result[s]/10;
            result[s]=result[s]%10;
            s++;
        }
        t++;
        s = t;
    }
    s = number1 + number2;
    while(result[s]==0){
        s--;
    }
    for(int k =s;k>=0;k--)
    {
        printf("%d",result[k]);
    }
    return 0;
}
