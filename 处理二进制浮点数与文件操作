#include<stdio.h>
#include<stdlib.h>
void input()
{
    FILE* fp;
    if ((fp = fopen("float.dat", "wb")) == NULL)
    {
        printf("error");
        exit(-1);
    }
    float number[14];
    for (int k = 0; k < 10; k++) {
        scanf("%f", &number[k]);
        fprintf(fp, "%f %c", number[k], ' ');
    }
    fclose(fp);
}
void output()
{
    FILE* fp;
    if ((fp = fopen("float.dat", "rw")) == NULL)
    {
        printf("error");
        exit(-1);
    }
    float num[14];
    for (int i = 0; i < 10; i++)
    {
        fscanf(fp, "%f", &num[i]);
        int m, j, k;
        unsigned char* p = (unsigned char*)&num[i] + 3;
        for (m = 0; m < 4; m++)
        {
            j = *(p - m);
            for (k = 7; k >= 0; k--)
            {
                if (j & (1 << k)) {
                    printf("1");
                }
                else {
                    printf("0");
                }
            }
        }
        printf("\n");
    }
    fclose(fp);
}
int main()
{
    input();
    output();
}
