#include<stdio.h>
int fanzhuan(int x, int y,int num,int count);
int shuru(int* count);  // 通过指针返回位数
int mi(int y);
int shuchu(int num, int count);
int main() {
    int count;
    int x = shuru(&count);
    int k = 100000;
    for(int i = 1 ; i <= count; i++) {
        for (int j = i + 1; j <= count; j++) {
			k = (k<fanzhuan(i,j,x,count))?k:fanzhuan(i,j,x,count);
        }
	}
	shuchu(k, count);
    return 0;
}

int shuru(int* count) {
    int a[100];
    *count = 0;
    int c;
    while ((c = getchar()) != EOF && c != '\n' && *count < 100) {
        if (c == '0' || c == '1') {
            a[*count] = c - '0';
            (*count)++;
        }
    }

    int b = 0;
    for (int i = 0; i < *count; i++) {
        b = mi(*count - i - 1) * a[i] + b;
    }
    return b;
}

int mi(int y) {
    if (y <= 0) return 1;
    int result = 1;
    for (int j = 0; j < y; j++) {
        result = result * 2;
    }
    return result;
}
int fanzhuan(int x, int y,int num,int count) {
    int part_of;
    part_of = num >> (count - y);
	part_of = part_of & (mi(y - x + 1) - 1);
    int wei = y - x + 1;
	int reversed = 0;
    for (int i = 0; i < wei; i++) {
        reversed = (reversed << 1) | (part_of & 1);
        part_of >>= 1;
    }
    int mask = (mi(wei) - 1) << (count - y);
    int result = (num & ~mask) | (reversed << (count - y));
	return result;
}
int shuchu(int num, int count) {
    int a[100];
    for (int i = 0; i < count; i++) {
        a[count - i - 1] = num % 2;
        num = num / 2;
    }
    for (int i = 0; i < count; i++) {
        putchar(a[i] + '0');
    }
    return 0;
}
